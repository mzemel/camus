#!/usr/bin/env ruby

require "bundler/setup"
require "camus"
require 'poseidon'
require 'snappy'

producer = Poseidon::Producer.new(["192.168.59.103:32780"], "my_test_producer")

messages = []
messages << Poseidon::MessageToSend.new("events", "test_value")
messages << Poseidon::MessageToSend.new("salads", "other_value")
producer.send_messages(messages)